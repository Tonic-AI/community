---
export interface Props {
    data?: string;
}
const { data } = Astro.props;
---
{

    data == "demo" && (
        <style>
            
            .clip-polygon {
                clip-path: polygon(0 0, 100% 0, 100% 100%, 0 calc(100% - 6vw));
            }
            
        </style>
    )

}