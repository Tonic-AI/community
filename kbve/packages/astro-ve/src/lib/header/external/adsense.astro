---
export interface Props {
	data?: string;
	widget?: string;
}
const { data, widget } = Astro.props;

let adsense = `https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-${data}`;
---

{
	(
		<>
			<script is:inline async src={adsense} crossorigin="anonymous" />
		</>
	)
}
